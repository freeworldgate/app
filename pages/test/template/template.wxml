    </view>
    <view class="rcc">
        <view class="rcc button" bindtap='dialog13'>选项卡1:顶部-X轴排列</view>
    </view>
    <view class="rcc">
        <view class="rcc button" bindtap='dialog14'>选项卡1:顶部-Y轴排列</view>
    </view>
    <view class="rcc">
        <view class="rcc button" bindtap='dialog15'>选项卡1:左侧-Y轴排列</view>
    </view>
    <view class="rcc">
        <view class="rcc button" bindtap='dialog16'>选项卡1:-右侧-Y轴排列</view>
    </view>
    <view class="rcc">
        <view class="rcc button" bindtap='dialog17'>选项卡1:-底部-X轴排列</view>
