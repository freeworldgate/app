    <view class="rcc">
        <view class="rcc button" bindtap='dialog3'>相册编辑窗口</view>
    </view>

    <view class="rcc">
        <view class="rcc button" bindtap='dialog4'>多行文本编辑</view>
    </view>
    <view class="rcc">
        <view class="rcc button" bindtap='dialog5'>单行文本编辑</view>
