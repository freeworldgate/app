                        <view class="h-20"></view>
                        <view class="rbc-nowrap ">
                            <view class="rcc">
                                <image class="w-35 h-35 br-35" mode="aspectFit" src="{{post.creator.imgUrl}}"></image>
                            </view>
                            <view class="w-10"></view>
                            <view class="css" style="flex:1">
                                <view class="rus line-1-start fs-20 fw-400 c-4 ">{{post.creator.userName}}</view>
                            </view>
                            <view class="w-20"></view>
                            <view class="rcc">
                                <image class="rcc w-25 h-25 " mode="aspectFit" src="/images/view3.png"></image>
                                <view class="rcc fs-20 fw-400 c-4">769</view>
                            </view>
                            <view class="w-20"></view>
                            <view class="rcc ">
                                <image class="rcc w-25 h-25 " mode="aspectFit" src="/images/rokit2.png"></image>
                                <view class="rcc fs-20 fw-400 c-4">769</view>
                            </view>
                        </view>
                    </view>
                    <view class="rbc absolute" style="width:48.5%">
                        <view class="rcc p-20 ">
                            <view class="rcc fs-25 fw-700 c-white h-50 w-50 br-50 b-45 z4 ">{{post.postImages.length}}</view>
                        </view>
                        <view class="rcc">
                            <view bindtap="likeOrDisLike" class="rcc p-20 z4" data-index="{{bindex}}" data-postid="{{post.postId}}">
                                <view class="rcc" wx:if="{{post.queryerCollect}}">
                                    <image class="rcc w-70 h-70 " mode="aspectFit" src="/images/rokit3.png"></image>
                                </view>
                                <view class="rcc" wx:else>
                                    <image class="rcc w-70 h-70 " mode="aspectFit" src="/images/rokit4.png"></image>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
                <view class="h-1vw"></view>
            </view>
        </view>
        <view class="w-1vw"></view>
        <view class="css" style="flex:1">
            <view class="css" hidden="{{bindex%2==0}}" wx:for="{{posts}}" wx:for-index="bindex" wx:for-item="post" wx:key="postkey">
                <view class="css b-white br-10 z2">
                    <view bindtap="showImgs" class="css z3" data-imgs="{{post.postImages}}">
                        <image bindtap="_post_show" class="css t-br-10" data-index="0" mode="widthFix" src="{{post.postImages[0].imgUrl}}" style="height:auto;width:auto;max-height:200vw"></image>
                    </view>
                    <view bindtap="showPost" class="css  pl-3vw pr-3vw pt-20 pb-20 " data-bindex="{{bindex}}" data-post="{{post}}">
                        <view class="rus line-2-start fs-30 fw-600  ">{{post.topic}}</view>
                        <view class="h-20"></view>
                        <view class="rbc-nowrap ">
                            <view class="rcc">
                                <image class="w-35 h-35 br-35" mode="aspectFit" src="{{post.creator.imgUrl}}"></image>
                            </view>
                            <view class="w-10"></view>
                            <view class="css" style="flex:1">
                                <view class="rus line-1-start fs-20 fw-400 c-4 ">{{post.creator.userName}}</view>
                            </view>
                            <view class="w-20"></view>
                            <view class="rcc">
                                <image class="rcc w-25 h-25 " mode="aspectFit" src="/images/view3.png"></image>
                                <view class="rcc fs-20 fw-400 c-4">769</view>
                            </view>
                            <view class="w-20"></view>
                            <view class="rcc ">
                                <image class="rcc w-25 h-25 " mode="aspectFit" src="/images/rokit2.png"></image>
                                <view class="rcc fs-20 fw-400 c-4">769</view>
                            </view>
                        </view>
                    </view>
                    <view class="rbc absolute" style="width:48.5%">
                        <view class="rcc p-20 ">
                            <view class="rcc fs-25 fw-700 c-white h-50 w-50 br-50 b-45 z4 ">{{post.postImages.length}}</view>
                        </view>
                        <view class="rcc">
                            <view bindtap="likeOrDisLike" class="rcc p-20 z4" data-index="{{bindex}}" data-postid="{{post.postId}}">
                                <view class="rcc" wx:if="{{post.queryerCollect}}">
                                    <image class="rcc w-70 h-70 " mode="aspectFit" src="/images/rokit3.png"></image>
                                </view>
                                <view class="rcc" wx:else>
                                    <image class="rcc w-70 h-70 " mode="aspectFit" src="/images/rokit4.png"></image>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
                <view class="h-1vw"></view>
            </view>
        </view>
        <view class="w-1vw"></view>
    </view>
</view>
<view class="rcs z30 customBar" style="top:{{top}}px">
    <view class="rcc h-32px w-32px br-32px bar-back box2">
        <image class="w-45 h-45" mode="aspectFit" src="/images/homePage.png"></image>
    </view>
    <view class="w-20"></view>
    <view class="rcc h-32px w-32px br-32px bar-back box2">
        <image class="w-45 h-45" mode="aspectFit" src="/images/share3.png"></image>
    </view>
</view>

<view class="rbc bottomBar p-30 b-white z30">
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="userFeeCode">上传收款码</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="applyOrder">applyOrder</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="userFeeOrder">收款</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="userPayOrder">付款</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="createFeeDialog">订单</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="createApproveDialog">审批收款码</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="createPostApproveDialog">审批POST</view>
    <view class="fs-20 fw-700 c-5 pt-30 pb-30" bindtap="createIntegralDialog">积分</view>
