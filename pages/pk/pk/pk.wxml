                    <view bindtap="_taskDialog_selectType" class="{{taskDialog.type===1?'rcc b-43 pl-20 pr-20 pt-10 pb-10':'rcc pl-20 pr-20 pt-10 pb-10'}}" data-type="1">
                        <view class="{{taskDialog.type===1?'rcc fs-20 fw-400 c-5':'rcc fs-20 fw-400 c-3-1'}}">打赏中</view>
                        <view class="rcc fs-18 line-h18 fw-400 c-1  ">+99</view>
                    </view>
                    <view class="w-30"></view>
                    <view bindtap="_taskDialog_selectType" class="{{taskDialog.type===2?'rcc b-43 pl-20 pr-20 pt-10 pb-10':'rcc pl-20 pr-20 pt-10 pb-10'}}" data-type="2">
                        <view class="{{taskDialog.type===2?'rcc fs-20 fw-400 c-5':'rcc fs-20 fw-400 c-3-1'}}">已打赏</view>
                        <view class="rcc fs-18 line-h18 fw-400 c-1  ">+99</view>
                    </view>
                </view>
                <view class="rcc fs-25 fw-400 c-5">积分排名前10%</view>
            </view>
        </view>
    </view>
    <view bindtap="_integralDialog_close" catchtouchmove="catchReturn" class="coverAll3" hidden="{{integralDialog.visible!=true}}"></view>
    <view catchtouchmove="catchReturn" class="css editDialog-3" hidden="{{integralDialog.visible!=true}}" style="left:{{integralDialog.left}}vw">
        <view class="css pl-4vw pr-4vw z30 b-white br-20">
            <view class="rbc pt-50 pb-20">
                <view class="rcs">
                    <image class="w-40 h-40 br-10" mode="aspectFit" src="/images/rokit.png"></image>
                    <view class="rcs">
                        <view class="rcs fs-30 fw-700 c-5" wx:if="{{integralDialog.integral.sort}}">排名:{{integralDialog.integral.index}}</view>
                        <view class="rcs fs-25 fw-400 c-5" wx:else>未参与排名,请上传收款码</view>
                    </view>
                </view>
                <view bindtap="share" class="rcc w-80 ">
                    <view class="rcc">
                        <view class="rcc ">
                            <image class="rcc w-40 h-40 " mode="aspectFit" src="/images/clickShare.png"></image>
                        </view>
                    </view>
                </view>
            </view>
            <view class="rac pt-50 pb-30 b-line">
                <view class="ccc">
                    <view class="fs-30 fw-400">打榜积分</view>
                    <view class="fs-60 fw-700 c-5">{{integralDialog.integral.follow}}</view>
                </view>
                <view class="ccc">
                    <view class="fs-30 fw-400">推广积分</view>
                    <view class="fs-60 fw-700 c-5">{{integralDialog.integral.share}}</view>
                </view>
            </view>
            <view class="rcs pt-30 pb-30">
                <image class="w-30 h-30 pr-20" mode="aspectFit" src="/images/laba.png"></image>
                <view class="fs-25 fw-400 c-3">推广积分每日清零，分享好友可以增加推广积分 ...</view>
            </view>
        </view>
    </view>
</view>



