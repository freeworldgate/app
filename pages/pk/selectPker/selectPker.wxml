<!--pages/pk/selectPker/selectPker.wxml-->
<view class="css">


            <view class="css pr-3vw pl-3vw"  >
                  <view class="css br-20 b-white mt-4vw pr-3vw pl-3vw">
                                    <view class="rbc ">

                                        <view class="rcc">
                                            <image class="w-25 h-25" mode="aspectFit" src="/images/time.png"></image>
                                            <view class="w-10"></view>
                                            <view class="rus fs-25 fw-400 c-3">{{pk.time}}</view>
                                        </view>




                                        <view class="rcc pt-40 pb-40"  data-pkid="{{pk.pkId}}" bindtap="viewPk">
                                            <view class="rcc fs-25 fw-400 c-3">{{pk.pkStatu.name}}</view>
                                            <!-- <image class="w-30 h-30" mode="aspectFit" src="/images/more.png"></image> -->
                                        </view>
                                    </view>
                                    <view class="rss" data-pkid="{{pk.pkId}}" bindtap="viewPk">
                                        <view class="rcc">
                                            <image class="w-150 h-150 br-10" mode="aspectFill" src="{{pk.user.imgUrl}}"></image>
                                        </view>
                                        <view class="w-3vw"></view>
                                        <view class="cas" style="flex:1">
                                            <view class="css"><view class="rus line-1-start fs-35 fw-700">{{pk.topic}}</view></view>
                                            <view class="css"><view class="rus line-2-start fs-25 fw-400">{{pk.watchWord}}</view></view>
                                        </view>
                                    </view>

                                    <view hidden="{{!pk.approveMessage}}" class="css"  data-pkid="{{pk.pkId}}" bindtap="approverMessageDetail">
                                                                            <view class="h-30 b-line"></view>
                                                                            <view class="h-30"></view>
                                                                            <view class="rss">
                                                                                <view class="css" style="flex:1">
                                                                                    
                                                                                    <view class="rbc pb-10">
                                                                                            <view class="rcc">
                                                                                                <view class="rcc w-40 h-40">
                                                                                                    <image class="w-35 h-35" mode="aspectFit" src="/images/statu1.png"></image>
                                                                                                </view>
                                                                                                <view class="w-20"></view>
                                                                                                <view class="rcc fs-25 fw-400 c-3">审核公告({{pk.invite}})</view>
                                                                                            
                                                                                            </view>
                                                                                            <view class="rcc">
                                                                                                <!-- <view class="rcc fs-25 fw-400 c-3">{{pk.invite}}</view> -->
                                                                                                
                                                                                            </view>

                                                                                            
                                                                                    </view>
                                                                                    <view class="css  pl-60 ">

                                                                                    
                                                                                            <view class="css">
                                                                    
                                                                                                    <view class="rus fs-30 fw-400 line-2-start c-word-6">{{pk.approveMessage.text}}</view>
                                                                                    
                                                                                            </view>    
                                                                                        
                                                                                    </view>  
                                                                                </view>
                                                                                <view class="w-20"></view>
                                                                                <view class="rcc">
                                                                                    <view class="rcc h-150 w-150 br-15" style="background-position: top;background-image: url({{pk.approveMessage.imgeUrl}});background-repeat: no-repeat;background-size: 100% auto; "></view>
                                                                                </view>
                                                                                <view class="w-20"></view>
                                                                                <view class="rcc"> <image class="w-30 h-30" mode="aspectFit" src="/images/right3.png"></image></view>
                                                                            </view>
 
                                                                            
                                                                            
                                                                            <view class="h-30 b-line"></view>                                                              
                                    </view>

                                    <!-- <view class="rcc fs-30 fw-700 p-100">{{pk.topic}}-{{pk.user.userId}}</view> -->
                                    <view class="rbc c-word-7">

                                        <view class="rus fs-25 fw-400 max-w-150 line-1-start">{{pk.user.userName}}</view>
                                        <view hidden="{{!pk.approved}}" class="rus fs-25 fw-400 ">{{pk.approved}}</view>
                                        <view hidden="{{!pk.approving}}" class="rus fs-25 fw-400 ">{{pk.approving}}</view>


                                        <view class="rcc"></view>
                                        <view class="rcc"></view>
                                        <view class="rcc pt-40 pb-40" bindtap="{{pk.groupInfo.mode===1?'groupCode':''}}" data-pkid="{{pk.pkId}}">
                                            <image hidden="{{pk.groupInfo.mode === 0}}" class="w-30 h-30" mode="aspectFit" src="{{pk.groupInfo.iconUrl}}"></image>
                                            <view hidden="{{pk.groupInfo.mode === 0}}" class="w-10"></view>
                                            <view class="rus fs-25 fw-400 ">{{pk.groupInfo.name}}</view>
                                        </view>
                                    </view>    
                  </view>
                
            </view>  





</view>





<view>
    <view class="coverAll" hidden="{{dialog.visible!=true}}"></view>
    <view class="css dialog " hidden="{{dialog.visible!=true}}">
        <view class="css dialog-padding">
            <view class="rcs t21-8-1 ">{{dialog.title}}</view>
            <view style="height:80rpx"></view>
            <view class="rus-wrap t21-8-2 ">{{dialog.text}}</view>
            <view style="height:80rpx"></view>
            <view class="rbc ">
                <view></view>
                <view bindtap="dialog_confirm" class="rcc shortTextDialog_confirm" hoverClass="dialog_confirm_hoverClass">确定</view>
            </view>
        </view>
    </view>
    <view class="rcc tipDialog " hidden="{{tipDialog.visible!=true}}">
        <view class="ccc levelDialog_view">
            <view class="rcc">
                <image class="tipDialog_image" mode="aspectFit" src="/images/warn.png" wx:if="{{tipDialog.level==='warn'}}"></image>
                <image class="tipDialog_image" mode="aspectFit" src="/images/error.png" wx:elif="{{tipDialog.level==='error'}}"></image>
                <image class="tipDialog_image" mode="aspectFit" src="/images/info.png" wx:else></image>
            </view>
            <view style="height:30rpx"></view>
            <view class="rcc levelDialog_text">{{tipDialog.message}}</view>
        </view>
    </view>
    <view class="coverAll" hidden="{{operateDialog.visible!=true}}"></view>
    <view class="css dialog " hidden="{{operateDialog.visible!=true}}">
        <view class="css dialog-padding">
            <view class="rcs t21-8-1 ">{{operateDialog.title}}</view>
            <view style="height:80rpx"></view>
            <view class="rus-wrap t21-8-2 ">{{operateDialog.text}}</view>
            <view style="height:80rpx"></view>
            <view class="rbc">
                <view bindtap="operateDialog_cancel" class="rcc shortTextDialog_cancel" hoverClass="dialog_cancel_hoverClass">取消</view>
                <view bindtap="operateDialog_confirm" class="rcc shortTextDialog_confirm" hoverClass="dialog_confirm_hoverClass">确定</view>
            </view>
        </view>
    </view>
    <view class="cac pageLoadingDialog b-48" hidden="{{pageLoadingDialog.visible!=true}}">
        <view class="ccc ">
            <view class="rcc">
                <image class="pageLoadingDialog_image" mode="aspectFit" src="/images/service.png"></image>
            </view>
            <view class="rcc"></view>
        </view>
        <view></view>
    </view>
    <view class="cac pageLoadingDialog b-48" hidden="{{pageLoadingDialogError.visible!=true}}">
        <view></view>
        <view class="ccc ">
            <view class="rcc">
                <image class="pageLoadingDialog_image" mode="aspectFit" src="/images/service.png"></image>
            </view>
            <view class="rcc"></view>
        </view>
    </view>
    <view catchtouchmove="return" class="coverAll99" hidden="{{labelLoadingDialog.visible!=true}}"></view>
    <view catchtouchmove="return" class="rcc labelLoadingDialog " hidden="{{labelLoadingDialog.visible!=true}}">
        <view class="rcc labelLoadingDialog_view">
            <image class="labelLoadingDialog_image" mode="aspectFill" src="/images/loading.gif"></image>
            <view class="labelLoadingDialog_text">加载中</view>
        </view>
    </view>
    <view class="rcc labelLoadingDialog " hidden="{{labelLoadingDialogSuccess.visible!=true}}">
        <view class="rcc labelLoadingDialog_view">
            <view class="rcc">
                <image class="labelLoadingDialog_image" mode="aspectFit" src="/images/loadsuccess.png"></image>
            </view>
            <view class="labelLoadingDialog_text">加载成功</view>
        </view>
    </view>
    <view class="rcc labelLoadingDialog " hidden="{{labelLoadingDialogError.visible!=true}}">
        <view class="rcc labelLoadingDialog_view">
            <view class="rcc">
                <image class="labelLoadingDialog_image" mode="aspectFit" src="/images/loadfail.png"></image>
            </view>
            <view class="labelLoadingDialog_text">加载失败</view>
        </view>
    </view>
    <view bindtap="_selectionDialog_hide" class="coverAll" hidden="{{selectionDialog.visible!=true}}"></view>
    <view class="{{selectionDialog._selectionDialog_layout}}" hidden="{{selectionDialog.visible!=true}}">
        <view class="css selectionDialog_BT " wx:if="{{selectionDialog.style===1||selectionDialog.style===5}}">
            <view class="rcc selectionDialog_BT1_title">选项</view>
            <view style="height:20rpx"></view>
            <view class="rac">
                <view bindtap="_selectDialog_choose" class="ccc" data-name="{{item.name}}" wx:for="{{selectionDialog.items}}" wx:key="*idx1">
                    <image class="selectionDialog_item_image" mode="aspectFit" src="{{item.icon}}"></image>
                    <view style="height:20rpx"></view>
                    <view class="selectionDialog_BT1_title">{{item.name}}</view>
                </view>
            </view>
        </view>
        <view class="css selectionDialog_RL" wx:elif="{{selectionDialog.style===2||selectionDialog.style===3}}">
            <view class="rcc selectionDialog_RL_title">选项</view>
            <view bindtap="_selectDialog_choose" class="ccc selectionDialog_RL_item" data-name="{{item.name}}" wx:for="{{selectionDialog.items}}" wx:key="*idx1">
                <image class="selectionDialog_item_image" mode="aspectFit" src="{{item.icon}}"></image>
                <view style="height:10rpx"></view>
                <view class="selectionDialog_RL_title">{{item.name}}</view>
            </view>
        </view>
        <view class="css selectionDialog_BT" wx:else>
            <view class="rcs selectionDialog_BT2_title">选项</view>
            <view bindtap="_selectDialog_choose" class="rcs selectionDialog_BT2_item" data-name="{{item.name}}" wx:for="{{selectionDialog.items}}" wx:key="idx1">
                <image class="selectionDialog_item_image" mode="aspectFit" src="{{item.icon}}"></image>
                <view style="width:30rpx"></view>
                <view class="selectionDialog_BT1_title">{{item.name}}</view>
            </view>
        </view>
    </view>












</view>